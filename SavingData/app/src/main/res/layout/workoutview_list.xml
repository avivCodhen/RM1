<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/workoutview_list_layout">

    <FrameLayout
        android:id="@+id/workoutview_list_shared_element_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></FrameLayout>

    <com.strongest.savingdata.MyViews.SmartEmptyView
        android:visibility="gone"
        android:id="@+id/workout_view_smartemptyview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></com.strongest.savingdata.MyViews.SmartEmptyView>
    <android.support.v7.widget.RecyclerView
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        android:id="@+id/workoutview_list_recycler"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></android.support.v7.widget.RecyclerView>


</RelativeLayout>

